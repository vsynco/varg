<%- include('../header.ejs') %>


        <section id="contacto" class="projects-section">
            <h2>Contacto</h2>
            <div class="projects">
                <div class="project">
                    <div class="project-header">
                            <h3>Formulario de Contacto</h3>
                    </div>
                    <form action="#" method="POST">
                        <div class="skills">
                            <div class="skill">
                                <span class="skill-name">Nombre</span>
                                <input type="text" name="name" required style="width: 100%; background-color: #282a36; color: #f8f8f2; border: 1px solid #6272a4; padding: 5px; border-radius: 4px;">
                            </div>
                            <div class="skill">
                                <span class="skill-name">Email</span>
                                <input type="email" name="email" required style="width: 100%; background-color: #282a36; color: #f8f8f2; border: 1px solid #6272a4; padding: 5px; border-radius: 4px;">
                            </div>
                            <div class="skill">
                                <span class="skill-name">Mensaje</span>
                                <textarea name="message" required style="width: 100%; background-color: #282a36; color: #f8f8f2; border: 1px solid #6272a4; padding: 5px; border-radius: 4px; height: 100px; resize: vertical;"></textarea>
                            </div>
                        </div>
                        <a href="#" class="project-link" onclick="this.closest('form').submit(); return false;">Enviar mensaje</a>
                    </form>
                </div>
            </div>
        </section>
        
  

<%- include('../footer.ejs') %>
